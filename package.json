        "prettier": "npx prettier --write ./",
        "postinstall": "husky install",
        "type-check": "tsc --project tsconfig.app.json --noEmit",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "storybook": "storybook dev -p 6006",
